{
  "name": "dubeer-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
  "build:shared": "npm run build -w packages/shared",
  "build:api": "npm run build:shared && npm run build -w apps/api",
  "start:api": "npm run start:prod -w apps/api",
  "seed:venues": "npm run seed:venues -w apps/api"
}

}
